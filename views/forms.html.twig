{% extends 'form_div_layout.html.twig' %}

{%- block form_errors -%}
    {%- if errors|length > 0 -%}
        <div>
            {%- for error in errors -%}
                <p class="uk-alert uk-alert-danger uk-text-small">{{ error.message }}</p>
            {%- endfor -%}
        </div>
    {%- endif -%}
{%- endblock form_errors -%}
